.version 6.5
.target sm_30
.address_size 64

.visible .entry reg_slm(
	.param .u64 input,
	.param .u64 output
)
{
    .local .align 8 .b8 slm[8];
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .b64 	    temp;
    .reg .s64 	    unused;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    mov.s64         unused, slm;

    ld.global.u64   temp, [in_addr];
    st.u64          [slm], temp;
    ld.u64          temp, [slm];
    st.global.u64   [out_addr], temp;
	ret;
}